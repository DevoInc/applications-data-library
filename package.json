    "eslint": "^5.14.1",
    "generate-changelog": "^1.7.1",
    "jest": "^23.6.0",
    "jest-fetch-mock": "^2.1.1",
    "karma": "^3.1.4",
    "karma-benchmark": "^1.0.1",
    "karma-benchmark-reporter": "^0.1.1",
    "karma-chrome-launcher": "^2.2.0",
